        </tr>
        <tr>
          <td></td>

          <td class="headerItem">Date:</td>
          <td class="headerValue">$date$</td>
          <td class="headerItem">Functions:</td>
          <td class="headerCovTableEntry">$report.coveredFunctionCount$</td>
          <td class="headerCovTableEntry">$report.calledFunctionCount$</td>
          <td class="headerCovTableEntry-$report.calledFunctionPercentageName$">$report.calledFunctionPercentage$ %</td>

        </tr>
        <tr>
          <td></td>

          <td class="headerItemLeg">Colors:</td>
          <td class="headerValueLeg">
            Line coverage:<br>
            <span class="coverLegendLo">0% to 15%</span>
            <span class="coverLegendMed">15% to 50%</span>
            <span class="coverLegendHi">50% to 100%</span>

          </td>
          <td></td>
          <td class="headerValueLeg" colspan=3>
            Function coverage:<br>
            <span class="coverLegendLo">0% to 75%</span>
            <span class="coverLegendMed">75% to 90%</span>
            <span class="coverLegendHi">90% to 100%</span>

          </td>
        </tr>

                <tr><td>&nbsp;</td></tr>
        </table>
      </td>
    </tr>
    <tr><td class="ruler">&nbsp;</td></tr>
  </table>

  <center>
  <table width="80%" cellpadding=1 cellspacing=1 border=0>

    <tr>
      <td width="45%"><br></td>
      <td width="15%"></td>
      <td width="10%"></td>
      <td width="10%"></td>
      <td width="10%"></td>

      <td width="10%"></td>
    </tr>

    <tr>
      <td class="tableHead">Directory<!-- <span class="tableHeadSort"><img src="../../glass.png" width=10 height=14 alt="Sort by name" title="Sort by name" border=0></span>--></td>
      <td class="tableHead" colspan=3>Line Coverage<!-- ( <a class="detail" href="index-detail.html">show details</a> ) <span class="tableHeadSort"><a href="index-sort-l.html"><img src="../../updown.png" width=10 height=14 alt="Sort by line coverage" title="Sort by line coverage" border=0></a></span>--></td>
      <td class="tableHead" colspan=2>Functions<!-- <span class="tableHeadSort"><a href="index-sort-f.html"><img src="../../updown.png" width=10 height=14 alt="Sort by function coverage" title="Sort by function coverage" border=0></a></span>--></td>

    </tr>
    $report.directoryReports:{
        <tr>
            <td class="coverFile"><a href="$it.directory;format="fullPath"$/index.html">$it.directory$</a></td>
            <td class="coverBar" align="center">
            <table border=0 cellspacing=0 cellpadding=1><tr><td class="coverBarOutline"><div style="width: 100px; height: 10px"><div class="progress-$it.calledLinePercentageName$" style="width: $it.calledLinePercentage$%; height: 100%"></div></div></td></tr></table>
            </td>
            <td class="coverPer-$it.calledLinePercentageName$">$it.calledLinePercentage$&nbsp;%</td>

            <td class="coverNum-$it.calledLinePercentageName$">$it.calledLineCount$ / $it.coveredLineCount$</td>
            <td class="coverPer-$it.calledFunctionPercentageName$">$it.calledFunctionPercentage$&nbsp;%</td>
            <td class="coverNum-$it.calledFunctionPercentageName$">$it.calledFunctionCount$ / $it.coveredFunctionCount$</td>
        </tr>
    }$
  </table>
