[
    {
        "settings": [ "master" ],
        "appPort": 8666,
        "shareYUIInstance":true,

        
        "staticHandling": {
            "prefix": "",
            "appName": "yahoo._template_.application",
            "frameworkName": "yahoo._template_.mojito"
        },

        "builds": {
            "html5app": {
                "urls": [
                    "/yahoo.application._template_/index.html"
                ]
            }
        }
    },
    {
        "settings": [ "environment:dev" ],
        
        "yui": {
            "base": "/yahoo.libs.yui/",
            "url": "$$yui.base$$yui/yui-min.js",
            "loader": "loader/loader-min.js"
        }
    }
]
