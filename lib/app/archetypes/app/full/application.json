[
    {
        "settings": [ "master" ],

        "appPort": 8666,
        "cacheViewTemplates": true,
        "deferAllOptionalAutoload": false,
        "embedJsFilesInHtmlFrame": false,
        "log": {
            "client": {
                "level": "info",
                "yui": false
            },
            "server": {
                "level": "info",
                "yui": false
            }
        },
        "shareYUIInstance": false,
        "staticHandling": {
            "appName": "app",
            "cache": false,
            "frameworkName": "mojito",
            "maxAge": 600000,
            "prefix": "static",
            "useRollups": false
        },
        "tunnelPrefix": "/tunnel/",
        "yui": {
            "dependencyCalculations": "precomputed"
        },

        "specs": {
            "frame": {
                "type": "HTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "child": {

                    }
                }
            }

        }
    },
    {
        "settings": [ "environment:development" ],
        "staticHandling": {
            "forceUpdate": true
        }
    }
]
